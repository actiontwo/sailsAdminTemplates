section#profile.container  .page-header    h3.text-success Profile  table.table    thead      tr        td Username        td {{ user.email || 'empty' }}      tr        td Số điện thoại        td          a(href='#', editable-text='dataUpdate.phone') {{ user.phone || 'empty' }}      tr        td Tên hiển thị        td          a(href='#', editable-text='dataUpdate.displayName') {{ user.displayName || 'empty' }}      tr        td Mật khẩu        td          a(href='#', editable-text='dataUpdate.password') **********      tr        td Group        td          ul.list-inline            li(            ng-repeat='item in user.groupId'            get-detail-attributes='{{item}}'            lists='userGroups' attribute='name')  div.col-md-12    .form-group      include ../modules/save-button