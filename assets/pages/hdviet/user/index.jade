section#dashboard-user.container  h4    a(ui-sref='find-user') Search User  div.row    div.col-md-12      div.panel.panel-default        .panel-heading          h3.panel-title THÔNG TIN TÀI KHOẢN        .panel-body          div.row            div.col-md-2              img(src='{{user.avatar || "https://placeholdit.imgix.net/~text?txtsize=30&txt=Avatar&w=100&h=100"}}' height=100)            div.col-md-5              table.table                tbody                  tr                    th Mã nhận diện user                    td {{user.uu_code}}                  tr                    th Tên                    td.text-success {{user.display_name}}                  tr                    th Ngày tạo tài khoản                    td {{user.created_date*1000 | date : 'dd/MM/yyyy' : 07}}            div.col-md-5              table.table                tbody                  tr                    th Loại tài khoản                    td(detect-account-type='{{user.open_id}}')                  tr                    th Tình trạng                    td(detech-user-blocked-type)                  tr                    th Số ngày Vip                    td {{user.vip}}  ul.nav.nav-tabs    li(ng-repeat='menu in menus' ui-sref-active="active")      a(ui-sref="{{menu.href}}"  ) {{menu.title}}  div#tabContent.tab-content    br    div#tab-1.tab-pane.fade.active.in(ui-view="")      //- include user-vip-info